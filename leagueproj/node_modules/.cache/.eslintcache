build\">\r\n            {/* SUMMONER SPELLS */}\r\n            <ul className=\"summs\">\r\n              <li>\r\n                <img className=\"summ-icon\" width=\"35\" height=\"35\"\r\n                src={`http://ddragon.leagueoflegends.com/cdn/13.9.1/img/spell/${getSpellIcon(selectedPlayer.summoner1Id)}.png`} alt=\"summ1\" />\r\n              </li>\r\n              <li>\r\n                <img className=\"summ-icon\" width=\"35\" height=\"35\"\r\n                src={`http://ddragon.leagueoflegends.com/cdn/13.9.1/img/spell/${getSpellIcon(selectedPlayer.summoner2Id)}.png`} alt=\"summ2\" />\r\n              </li>\r\n            </ul>\r\n            {/* RUNES */}\r\n            <ul className=\"runes\">\r\n              <li>\r\n                <img width=\"35\" height=\"35\"\r\n                src={`https://ddragon.canisback.com/img/${getPrimaryRune(selectedPlayer.perks.styles)}`} alt=\"keystone\" />\r\n              </li>\r\n              <li>\r\n                <img width=\"35\" height=\"35\"\r\n                src={`https://ddragon.canisback.com/img/${getSecondaryRune(selectedPlayer.perks.styles)}`} alt=\"rune\" />\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n        <Score selectedPlayer={selectedPlayer} />\r\n      </div>\r\n    </>\r\n  )\r\n}\r\n\r\nexport default MatchStats","C:\\Users\\Gabriel\\Desktop\\Work\\leaguestats\\LeagueStats\\leagueproj\\src\\Components\\Score\\Score.js",[],[],{"ruleId":"56","severity":1,"message":"57","line":1,"column":17,"nodeType":"58","messageId":"59","endLine":1,"endColumn":25},{"ruleId":"60","severity":1,"message":"61","line":12,"column":47,"nodeType":"62","messageId":"63","endLine":12,"endColumn":49},{"ruleId":"60","severity":1,"message":"61","line":10,"column":49,"nodeType":"62","messageId":"63","endLine":10,"endColumn":51},{"ruleId":"60","severity":1,"message":"61","line":24,"column":25,"nodeType":"62","messageId":"63","endLine":24,"endColumn":27},{"ruleId":"60","severity":1,"messag